# API Keys
# Amadeus API (for flights and hotels) - Get from: https://developers.amadeus.com/
VITE_AMADEUS_API_KEY=your_amadeus_api_key
VITE_AMADEUS_API_SECRET=your_amadeus_api_secret
# For Netlify functions (without VITE_ prefix)
AMADEUS_API_KEY=your_amadeus_api_key
AMADEUS_API_SECRET=your_amadeus_api_secret
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_OPENAI_API_KEY=your_openai_api_key
# Or use DeepSeek instead
# VITE_DEEPSEEK_API_KEY=your_deepseek_api_key

# Supabase
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Places API (for venue information)
# Get your API key from: https://console.cloud.google.com/apis/credentials
# Enable Places API (Text Search) and Places API (Place Details)
VITE_GOOGLE_PLACES_API_KEY=your_google_places_api_key_here

# Reddit API (optional - for enhanced recommendations)
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# Analytics
VITE_GA_TRACKING_ID=your_google_analytics_id_here

# Affiliate IDs
VITE_BOOKING_AFFILIATE_ID=your_booking_affiliate_id
VITE_VIATOR_AFFILIATE_ID=your_viator_affiliate_id

# App Configuration
VITE_APP_URL=http://localhost:3000
VITE_API_URL=/.netlify/functions

# Development Settings
NODE_ENV=development
VITE_DEBUG_MODE=true 